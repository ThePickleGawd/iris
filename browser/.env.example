# Browser service settings
BROWSER_SERVICE_PORT=8010
BROWSER_SERVICE_HOST=0.0.0.0

# Stagehand runtime: LOCAL (visible local browser windows) or BROWSERBASE (cloud browser)
STAGEHAND_ENV=LOCAL

# Browserbase credentials (required only when STAGEHAND_ENV=BROWSERBASE)
BROWSERBASE_API_KEY=
BROWSERBASE_PROJECT_ID=

# LLM key for Stagehand agenting
ANTHROPIC_API_KEY=

# Optional Stagehand model/runtime overrides
BROWSER_PRIMARY_ENGINE=browser_use
STAGEHAND_MODEL=anthropic/claude-3-7-sonnet-latest
BROWSER_STAGEHAND_TIMEOUT_MS=60000
BROWSER_STAGEHAND_HEADLESS=false
BROWSER_STAGEHAND_ENABLE_AGENT=false
BROWSER_STAGEHAND_AGENT_MODE=hybrid
BROWSER_STAGEHAND_RETRIES=1
BROWSER_STAGEHAND_RETRY_BACKOFF_MS=1200
BROWSER_STAGEHAND_EXECUTION_STRATEGY=deterministic_first
BROWSER_STAGEHAND_INIT_TIMEOUT_MS=20000
BROWSER_STAGEHAND_NAV_TIMEOUT_MS=30000
BROWSER_STAGEHAND_ACT_TIMEOUT_MS=25000
BROWSER_STAGEHAND_AGENT_TIMEOUT_MS=35000
BROWSER_STAGEHAND_DISABLE_API=true
BROWSER_STAGEHAND_SELF_HEAL=true
BROWSER_STAGEHAND_DOM_SETTLE_TIMEOUT_MS=1800
BROWSER_ENABLE_BROWSER_USE_FALLBACK=true

# Optional: override system prompt source file for Stagehand agent behavior.
# Defaults to repo-root skills.md
BROWSER_SYSTEM_PROMPT_PATH=
